<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="fragment.HomeFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <com.astuetz.PagerSlidingTabStrip
            android:layout_width="match_parent"
            android:layout_height="48dp"
            xmlns:myall="http://schemas.android.com/apk/res/com.example.administrator.myall"
            myall:pstsIndicatorColor="@color/background_tab_pressed"
            myall:pstsIndicatorHeight="5dp"
            android:textSize="18dp"
            android:id="@+id/indicator"
            />

        <android.support.v4.view.ViewPager android:id="@+id/home_view_pager"

            android:layout_width="match_parent"
            android:layout_height="match_parent">


        </android.support.v4.view.ViewPager>

    </LinearLayout>


</FrameLayout>
